<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testrequests">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestRequests">
        <activity android:name="com.example.view.home.StaffHomeActivity"></activity>
        <activity android:name="com.example.view.home.CustomerHomeActivity" />
        <activity android:name="com.example.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.view.authentication.LoginActivity" />
        <activity android:name="com.example.view.authentication.SignUpActivity" />
        <activity android:name="com.example.view.LandingPageActivity" />
    </application>

</manifest>